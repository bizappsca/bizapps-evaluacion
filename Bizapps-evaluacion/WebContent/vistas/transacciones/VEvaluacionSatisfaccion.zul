						<listheader label="MUY BUENO" />
						<listheader label="BUENO" />
						<listheader label="REGULAR" />
						<listheader label="MALO" />
						<listheader label="MUY MALO" />
					</listhead>
					<template name="model">
						<listitem>
							<listcell>

