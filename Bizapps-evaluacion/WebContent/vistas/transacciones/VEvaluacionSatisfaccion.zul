							</listcell>
							<listcell>
								<checkbox />
							</listcell>
							<listcell>
								<checkbox />
							</listcell>
							<listcell>
								<checkbox />
							</listcell>
						</listitem>
					</template>
				</listbox>
				<separator />
				<separator />
			</groupbox>
			<separator />
			<separator />
		</groupbox>
		<div id="divCatalogoCurso"
			tooltiptext="Click para Seleccionar un curso" />
	</window>
</zk>