<window id="wdwVMedicion" apply="controlador.maestros.CMedicion"
	border="normal" height="auto" style="background-color:#8A0808;" width="100%">

	<groupbox width="auto" mold="3d" title="Medición">
		<separator />
		<div id="botoneraMedicion" />
		<separator />
		<separator />
		<groupbox id="gpxRegistroMedicion" width="auto" title="" class = "azul"
			open="false">
			<caption label="Nuevo" style="font-weight:bold; font-size: 13px"
				tooltiptext="Presione para registrar una nueva medicion" />
			<separator />
			<grid width="100%">
				<columns visible="false">
					<column label="" width="20%" />
					<column label="" width="55%" />
					<column label="" width="25%" />
				</columns>
				<rows>
					<row>
						<hbox>
							<label value="Descripción:"
								class="etiqueta" />
							<space />
							<label value="*"
								style="font-weight:bold;color:red">
							</label>
						</hbox>

						<textbox id="txtDescripcionMedicion" width="90%"
							maxlength="100" tabindex="1"
							tooltiptext="Ingrese aqui la descripcion de la medicion" />
							<hbox/>
					</row>
				</rows>
			</grid>
		</groupbox>
		<separator />
		<separator />
		<groupbox id="gpxDatosMedicion" width="auto" title="" class = "azul">
			<caption label="Registros" style="font-weight:bold; font-size: 13px"
				tooltiptext="Presione para visualizar las mediciones registradas" />
			<div id="catalogoMedicion"
				tooltiptext="Seleccione la medicion para realizar una accion" />
		</groupbox>
		<separator />
		<separator />
	</groupbox>
</window>